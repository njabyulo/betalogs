{
  "name": "@betalogs/shared",
  "version": "0.1.0",
  "private": true,
  "exports": {
    "./utils": {
      "import": "./dist/utils/index.mjs",
      "types": "./dist/utils/index.d.mts"
    },
    "./constants": {
      "import": "./dist/constants/index.mjs",
      "types": "./dist/constants/index.d.mts"
    },
    "./types": {
      "import": "./dist/types/index.mjs",
      "types": "./dist/types/index.d.mts"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
